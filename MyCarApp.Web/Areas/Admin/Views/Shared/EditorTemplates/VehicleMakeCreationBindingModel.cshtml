@model VehicleMakeCreationBindingModel
@{
    var attributes = new Dictionary<string, object>();

    attributes.Add("LabelFor", new { @class = "control-label col-sm-2" });

    attributes.Add("EditorFor", new { @class = "form-control", @placeholder = "Make Name" });

    attributes.Add("ValidationMessageFor", new { @class = "text-danger" });

}
@Html.FormEditGroupWithCustomAttibutesFor(m => m.Name, attributes)